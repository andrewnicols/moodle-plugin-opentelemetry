{
    "name": "moodle/moodle-collection-otel",
    "type": "project",
    "require": {
        "open-telemetry/sdk": "^1.10",
        "google/protobuf": "^4.31",
        "open-telemetry/exporter-otlp": "^1.3",
        "open-telemetry/opentelemetry-auto-curl": "^0.1.0",
        "open-telemetry/opentelemetry-auto-guzzle": "^1.2",
        "open-telemetry/opentelemetry-auto-http-async": "^1.1",
        "open-telemetry/opentelemetry-auto-mysqli": "^0.0.3",
        "open-telemetry/opentelemetry-auto-psr14": "^0.0.5",
        "open-telemetry/opentelemetry-auto-psr18": "^1.1",
        "open-telemetry/opentelemetry-auto-psr3": "^0.1.1",
        "open-telemetry/opentelemetry-auto-session": "^0.1.0",
        "open-telemetry/opentelemetry-logger-monolog": "^1.1",
        "open-telemetry/opentelemetry-sqlcommenter": "^0.1.0",
        "open-telemetry/transport-grpc": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Moodle\\Telemetry\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": false,
            "tbachert/spi": false
        }
    }
}
